@prefix : <http://purl.org/science/ontology/example/lumen.owl#> .
@prefix example: <http://purl.org/science/ontology/example/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix owl2xml: <http://www.w3.org/2006/12/owl2-xml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

example:LUME0000011 a owl:Class ;
    rdfs:label "Lumen of SubClavian Artery" ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( example:LUME0000001 [ a owl:Restriction ;
                        owl:onProperty [ a owl:ObjectProperty ;
                                owl:inverseOf example:LUME0000003 ] ;
                        owl:someValuesFrom example:LUME0000007 ] ) ] .

example:LUME0000012 a owl:Class ;
    rdfs:label "Continuous with the Lumen of Radial Artery" ;
    rdfs:subClassOf owl:Thing ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty example:LUME0000006 ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( example:LUME0000001 [ a owl:Restriction ;
                                owl:onProperty [ a owl:ObjectProperty ;
                                        owl:inverseOf example:LUME0000003 ] ;
                                owl:someValuesFrom example:LUME0000009 ] ) ] ] .

example:lumen.owl a owl:Ontology ;
    rdfs:comment "http://groups.google.com/group/fma-owl-2009/browse_thread/thread/4643f80187bf9327" .

owl:Thing a owl:Class .

example:LUME0000005 a owl:ObjectProperty ;
    rdfs:label "ArterySelf" .

example:LUME0000007 a owl:Class ;
    rdfs:label "SubClavianArtery" ;
    rdfs:comment """SubClavianArtery SubClassOf 
  (Artery and 
   (hasPart1 some Lumen) and 
   (hasPart1 some Wall) and 
   (isContinuousWith some RadialArtery))""" ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( example:LUME0000002 [ a owl:Restriction ;
                        owl:onProperty example:LUME0000003 ;
                        owl:someValuesFrom example:LUME0000001 ] [ a owl:Restriction ;
                        owl:onProperty example:LUME0000003 ;
                        owl:someValuesFrom example:LUME0000008 ] [ a owl:Restriction ;
                        owl:onProperty example:LUME0000006 ;
                        owl:someValuesFrom example:LUME0000009 ] ) ] .

example:LUME0000008 a owl:Class ;
    rdfs:label "Wall" .

example:LUME0000009 a owl:Class ;
    rdfs:label "RadialArtery" ;
    rdfs:subClassOf [ a owl:Class ;
            owl:intersectionOf ( example:LUME0000002 [ a owl:Restriction ;
                        owl:onProperty example:LUME0000003 ;
                        owl:someValuesFrom example:LUME0000001 ] [ a owl:Restriction ;
                        owl:onProperty example:LUME0000003 ;
                        owl:someValuesFrom example:LUME0000008 ] [ a owl:Restriction ;
                        owl:onProperty example:LUME0000006 ;
                        owl:someValuesFrom example:LUME0000007 ] ) ],
        example:LUME0000002 .

example:LUME0000002 a owl:Class ;
    rdfs:label "Artery" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasSelf true ;
            owl:onProperty example:LUME0000005 ] .

example:LUME0000004 a owl:ObjectProperty ;
    rdfs:label "LumenSelf" .

example:LUME0000001 a owl:Class ;
    rdfs:label "Lumen" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:hasSelf true ;
            owl:onProperty example:LUME0000004 ] .

example:LUME0000006 a owl:ObjectProperty ;
    rdfs:label "is continuous with" ;
    rdfs:comment """Then the following axiom 
(3) SubProperty(SubPropertyChain( LumenSelf InverseOf(hasPart1)   
isContinuousWith ArterySelf hasPart1 LumenSelf) isContinuousWith)""" ;
    owl:propertyChainAxiom ( example:LUME0000004 [ a owl:ObjectProperty ;
                owl:inverseOf example:LUME0000003 ] example:LUME0000006 example:LUME0000005 example:LUME0000003 example:LUME0000004 ) .

example:LUME0000003 a owl:ObjectProperty ;
    rdfs:label "has part" .

