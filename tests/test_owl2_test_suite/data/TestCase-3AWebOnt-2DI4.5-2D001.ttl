@prefix first: <http://www.w3.org/2002/03owlt/I4.5/premises001#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

first:EuroMP a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty first:isEuroMPFrom ;
            owl:someValuesFrom owl:Thing ] .

first:BE a first:EuropeanCountry .

first:ES a first:EuropeanCountry .

first:EUCountry a owl:Class ;
    owl:oneOf ( first:UK first:BE first:ES first:FR first:NL first:PT ) .

first:FR a first:EuropeanCountry .

first:Kinnock a first:Person .

first:NL a first:EuropeanCountry .

first:PT a first:EuropeanCountry .

first:Person a owl:Class .

first:UK a first:EuropeanCountry ;
    first:hasEuroMP first:Kinnock .

first:hasEuroMP a owl:ObjectProperty ;
    rdfs:domain first:EUCountry .

first:isEuroMPFrom a owl:ObjectProperty ;
    owl:inverseOf first:hasEuroMP .

first:EuropeanCountry a owl:Class .

[] a owl:Ontology .

