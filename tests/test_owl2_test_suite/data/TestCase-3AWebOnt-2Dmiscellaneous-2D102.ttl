@prefix first: <http://www.w3.org/2002/03owlt/miscellaneous/consistent102#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

first:i a [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:allValuesFrom first:a ;
                        owl:onProperty first:p ] [ a owl:Restriction ;
                        owl:onProperty first:p ;
                        owl:someValuesFrom first:s ] ) ],
        owl:Thing .

first:a a owl:Class .

first:s a owl:Class .

first:p a owl:ObjectProperty .

[] a owl:Ontology .

