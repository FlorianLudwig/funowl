first:A-and-B a owl:Class ;
    owl:unionOf ( first:A first:B ) .

first:A a owl:Class ;
    owl:oneOf ( first:a ) .

first:B a owl:Class ;
    owl:oneOf ( first:b ) .

first:a a owl:Thing .

first:b a owl:Thing .

[] a owl:Ontology .

