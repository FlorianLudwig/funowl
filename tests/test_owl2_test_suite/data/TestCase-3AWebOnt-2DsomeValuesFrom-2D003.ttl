@prefix first: <http://www.w3.org/2002/03owlt/someValuesFrom/premises003#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

first:fred a first:person .

first:parent a owl:ObjectProperty .

first:person a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty first:parent ;
            owl:someValuesFrom first:person ] .

[] a owl:Ontology .

