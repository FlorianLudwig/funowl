@prefix first: <http://www.w3.org/2002/03owlt/oneOf/premises004#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

first:i a [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:int ;
            owl:onProperty first:p ],
        owl:Thing .

first:p a owl:DatatypeProperty ;
    rdfs:range [ a owl:DataRange ;
            owl:oneOf [ a rdf:List ;
                    rdf:first 4 ;
                    rdf:rest [ a rdf:List ;
                            rdf:first 5 ;
                            rdf:rest [ a rdf:List ;
                                    rdf:first 6 ;
                                    rdf:rest () ] ] ] ],
        [ a owl:DataRange ;
            owl:oneOf [ a rdf:List ;
                    rdf:first 1 ;
                    rdf:rest [ a rdf:List ;
                            rdf:first 2 ;
                            rdf:rest [ a rdf:List ;
                                    rdf:first 3 ;
                                    rdf:rest [ a rdf:List ;
                                            rdf:first 4 ;
                                            rdf:rest () ] ] ] ] ] .

[] a owl:Ontology .

